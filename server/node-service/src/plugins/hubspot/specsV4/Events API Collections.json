  /events/v3/send:
    post:
      tags:
        - Custom Event Definitions API > Event tracking
      summary: Send custom event completion data
      description: Send event data to HubSpot using an existing custom event definition.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                eventName: <string>
                occurredAt: <dateTime>
                utk: <string>
                uuid: <string>
                email: <string>
                properties:
                  site: <string>
                objectId: <string>
      security:
        - apikeyAuth: []
      parameters:
        - name: Content-Type
          in: header
          schema:
            type: string
          example: application/json
        - name: Accept
          in: header
          schema:
            type: string
          example: '*/*'
      responses:
        '204':
          description: No Content
          content:
            text/plain:
              schema:
                type: string
              example: null
        '500':
          description: Internal Server Error
